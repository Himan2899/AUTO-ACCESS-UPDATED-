# NPM Configuration

# Save exact version of packages
save-exact=true

# Use package-lock.json
package-lock=true

# Use yarn.lock
yarn-lock=true

# Use npm audit
audit=true

# Use npm fund
fund=false

# Use npm update
update-notifier=true

# Use npm cache
cache-min=3600

# Use npm registry
registry=https://registry.npmjs.org/

# Use npm scope
scope=autoaccess

# Use npm access
access=public

# Use npm tag
tag=latest

# Use npm version
version-git-tag=true
version-git-sign=true
version-git-message="chore(release): %s"

# Use npm publish
publish-access=public

# Use npm dist-tag
dist-tag=latest

# Use npm pack
pack-destination=./dist

# Use npm run
run-script=true

# Use npm test
test-script=test

# Use npm start
start-script=start

# Use npm stop
stop-script=stop

# Use npm restart
restart-script=restart

# Use npm preinstall
preinstall-script=preinstall

# Use npm install
install-script=install

# Use npm postinstall
postinstall-script=postinstall

# Use npm preuninstall
preuninstall-script=preuninstall

# Use npm uninstall
uninstall-script=uninstall

# Use npm postuninstall
postuninstall-script=postuninstall

# Use npm preversion
preversion-script=preversion

# Use npm version
version-script=version

# Use npm postversion
postversion-script=postversion

# Use npm pretest
pretest-script=pretest

# Use npm test
test-script=test

# Use npm posttest
posttest-script=posttest

# Use npm prestop
prestop-script=prestop

# Use npm stop
stop-script=stop

# Use npm poststop
poststop-script=poststop

# Use npm prestart
prestart-script=prestart

